{
  "name": "my-new-app",
  "productName": "my-new-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Vazra"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "moctrash",
              "name": "electron-forge-test-1"
            },
            "prerelease": false
          }
        }
      ],
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_new_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "6.0.0-beta.50",
    "@types/faker": "4.1.12",
    "@types/jest": "24.9.1",
    "@types/node": "12.12.47",
    "@types/react": "^16.9.36",
    "@types/react-bootstrap-table-next": "4.0.4",
    "@types/react-bootstrap-table2-paginator": "2.1.1",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@zeit/webpack-asset-relocator-loader": "https://github.com/vazra/webpack-asset-relocator-loader",
    "css-loader": "^3.0.0",
    "electron": "9.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "node-loader": "^0.6.0",
    "source-map-loader": "^1.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "bootstrap": "4.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "faker": "4.1.0",
    "pouchdb-adapter-idb": "7.2.1",
    "pouchdb-adapter-leveldb": "^7.2.1",
    "pouchdb-adapter-memory": "7.2.1",
    "react": "^16.13.1",
    "react-bootstrap": "1.0.1",
    "react-bootstrap-table-next": "4.0.3",
    "react-bootstrap-table2-paginator": "2.1.2",
    "react-dom": "^16.13.1",
    "rxdb": "9.1.0",
    "rxjs": "6.5.5"
  }
}
